<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="Client">
	
	<select id="getClientListCount" resultType="_int">
		SELECT COUNT(*)
		FROM MEMBER
		WHERE TYPE = 'CLIENT'
	</select>
	
</mapper>































